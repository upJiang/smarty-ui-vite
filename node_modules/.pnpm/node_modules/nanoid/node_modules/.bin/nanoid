#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/jiang/smarty-ui-vite/node_modules/.pnpm/nanoid@3.3.4/node_modules/nanoid/bin/node_modules:/mnt/d/jiang/smarty-ui-vite/node_modules/.pnpm/nanoid@3.3.4/node_modules/nanoid/node_modules:/mnt/d/jiang/smarty-ui-vite/node_modules/.pnpm/nanoid@3.3.4/node_modules:/mnt/d/jiang/smarty-ui-vite/node_modules/.pnpm/node_modules:/mnt/d/jiang/smarty-ui-vite/node_modules:/mnt/d/jiang/node_modules:/mnt/d/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/jiang/smarty-ui-vite/node_modules/.pnpm/nanoid@3.3.4/node_modules/nanoid/bin/node_modules:/mnt/d/jiang/smarty-ui-vite/node_modules/.pnpm/nanoid@3.3.4/node_modules/nanoid/node_modules:/mnt/d/jiang/smarty-ui-vite/node_modules/.pnpm/nanoid@3.3.4/node_modules:/mnt/d/jiang/smarty-ui-vite/node_modules/.pnpm/node_modules:/mnt/d/jiang/smarty-ui-vite/node_modules:/mnt/d/jiang/node_modules:/mnt/d/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../bin/nanoid.cjs" "$@"
fi
